[Calendar]:
LOAD
    _kDate,
    "Date",
    "Month",
    "Year",
    FiscalYear,
    Period,
    CPeriod,
    Quarter,
    CQuarter,
    HalfYear,
    FMonthYear,
    CMonthYear,
    "Day",
    "Week",
    "Weekstart",
    "Weekend",
    "WeekDay",
    fFM,
    DailyReportDate,
    DailyReportDate.Day,
    fMTD,
    fYTD,
    fFYTD,
    MonthYear,
    "Fiscal HalfYear",
    "Calendar HalfYear",
    "Fiscal Quarter",
    "Calendar Quarter",
    QDay,
    YDay
FROM [lib://IBCS source (dvsts_d.bosyi)/Calendar.csv]
(txt, utf8, embedded labels, delimiter is ',', msq);

[Dataset]:
LOAD
    _kDate,
    Region,
    Market,
    Brand,
    "Customer Division",
    Sales,
    FxB_Sales,
    FC_Sales,
    _FxLYkey
FROM [lib://IBCS source (dvsts_d.bosyi)/DataSet.csv]
(txt, utf8, embedded labels, delimiter is ',', msq);

[FxLYSales]:
LOAD
    _FxLYkey,
    FxLY_Sales_FY16,
    FxLY_Sales_FY17,
    FxLY_Sales_FY18
FROM [lib://IBCS source (dvsts_d.bosyi)/FxLYSales.csv]
(txt, utf8, embedded labels, delimiter is ',', msq);

